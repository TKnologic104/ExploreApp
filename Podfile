  pod 'Firebase/Storage'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'

  target 'ExploreAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ExploreAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
